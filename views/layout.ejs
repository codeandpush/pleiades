<body style="position: relative">


<div id="connection_alert"
     style="background-color: orange;width: 100%;position: absolute;left: 0px;top: 0px;height: 32px">
    <center style="color: white"><h5>Connection lost, please refresh the page</h5></center>
</div>


<main style="height: 100%">
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a class="navbar-brand" data-emit-click="tab_home">PlayThis</a>
        <ul class="navbar-nav ml-auto">
            <button type="button" class="btn btn-link" data-emit-click="tab_signin">
                Sign In
            </button>
        </ul>
    </nav>
     <div class="tab-content" id="myTabContent">
         <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
             <%- include('_tab_home', {user: {nickname: 'TestUser'}}) %>
         </div>
     </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</main>


<script src="dist/bkendz.js"></script>
